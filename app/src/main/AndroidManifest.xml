<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="io.rcm.wicker">

  <application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:name=".base.WickerApp"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/BaseAppTheme">

    <activity android:name=".onboarding.presentation.OnboardingActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />
        <action android:name="android.intent.action.VIEW" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  
    <activity
        android:name=".quotes.features.details.presentation.QuoteDetailsActivity"/>
  
    <activity
      android:name=".quotes.features.writer.presentation.QuoteWriterActivity"
      android:theme="@style/BaseAppTheme.NoToolbar"/>

  </application>
</manifest>
